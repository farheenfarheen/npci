package javapractise;

import java.util.Scanner;

public class Matrix {

	public static void main(String args[])
	{
		int m=3,n=2;
		int a[][] =new int[n][m];
		Scanner sc= new Scanner(System.in);
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<m;j++)
			{
				a[i][j]=sc.nextInt();
			}
		}
		
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<m;j++)
			{
				System.out.print(a[i][j]+" ");
			}
			System.out.print("\n");
		}
		
		sc.close();
	}
}
